<?xml version="1.0" encoding="UTF-8"?>
<model name="hack:fingerprintModel" xmlns="http://www.alfresco.org/model/dictionary/1.0">
	<!-- Optional meta-data about the model -->
	<description>Hackathon Fingerprint Model</description>
	<author>Martin Cosgrave</author>
	<version>1.0</version><!-- Imports are required to allow references to definitions 
		in other models -->
	<imports><!-- Import Alfresco Dictionary Definitions -->
		<import uri="http://www.alfresco.org/model/dictionary/1.0"
			prefix="d" /><!-- Import Alfresco Content Domain Model Definitions -->
		<import uri="http://www.alfresco.org/model/content/1.0" prefix="cm" />
	</imports><!-- Introduction of new namespaces defined by this model -->
	<namespaces>
		<namespace uri="http://hackathon2013.alfresco.org/model/fingerprint/1.0"
			prefix="hack" />
	</namespaces>

	<aspects>
		<aspect name="hack:fingerprintDataAspect">
			<properties>
				<property name="hack:fingerprints">
					<title>Fingerprint data</title>
					<type>d:text</type>
					<multiple>true</multiple>
				</property>
			</properties>
		</aspect>

	</aspects>

</model>